((= Python input/output style =))

((*- extends 'base.tplx' -*))

% Custom definitions
((* block definitions *))
    ((( super() )))

    % Pygments definitions
    ((( resources.latex.pygments_definitions )))
((* endblock definitions *))

%===============================================================================
% Input
%===============================================================================

((* block input scoped *))
